<% if (state==0){ %>
<% var applyFormData=data.applyForm;var medicineData=data.medicine==undefined || data.medicine==''?'':data.medicine;
   var templetData=data.templet;var iH=templetData.historyNum==undefined||templetData.historyNum==''?'':eval('(' + templetData.historyNum + ')');var iHs=templetData.recentNum==undefined||templetData.recentNum==''?'':eval('(' + templetData.recentNum + ')');
%>
<section class="Ynav temNav">
    <dfn
            <% if (ifPatient=='1'){ %>
            isHref="J_docAdviceList.html?memberId=<%=memberId%>&ifPatient=1"
            <% } else {%>
            isHref="J_docAdviceList.html?memberId=<%=memberId%>&myPatient=1"
            <% } %>

            class="a Y_tump adviceBack J_jumpBtnIback"> </dfn>
    <h1 class="category">高血压咨询模板</h1>
    <i class="temOk ifSaveBtn">发送</i>
    <span class="temSave ifSaveBtn">保存</span>
</section>
<div class="Y_docIndex JindexBox JindexBoxTop">
    <form class="TNB_listForm JDCCon">
        <input type="hidden" id="familyImAccid" name="familyImAccid" value="<%=data.familyImAccid%>">
        <input type="hidden" name="applyFormId" value="<%=applyFormData.id%>"/>
        <div class="Teminput_item Teminput_Base">
            <h4 class="clearfix">
                <label>问诊时间</label>
                <div class="form_right">
                    <input type="text" name="startTime" class="startTime ifNullInp" value="<%=templetData.startTime%>"/>
                    <em class="huiRight"></em>
                </div>
            </h4>
            <h4 class="clearfix">
                <label>患病年限</label>
                <div class="form_right">
                    <input type="number" name="sickYears" class="sickYears ifNullInp" placeholder="请输入患病年限" value="<%=templetData.sickYears%>"/>
                    <em>年</em>
                </div>
            </h4>
            <h4 class="clearfix">
                <label>确诊医院</label>
                <div class="form_right">
                    <input type="text"  name="hospitalName"  class="hospitalName ifNullInp" placeholder="请输入确诊医院" value="<%=templetData.hospitalName%>"/>
                </div>
            </h4>
            <h4 class="clearfix form_block">
                <label>本次咨询原因</label>
                <div class="form_right">
                    <textarea contenteditable="true" name="askReason" class="askReason ifNullInp" value="<%=templetData.askReason%>"  placeholder="请输入本次咨询原因"><%=templetData.askReason%> </textarea>
                </div>
            </h4>
        </div>
        <div class="Teminput_item">
            <h4 class="clearfix item_title">
                <label>血压情况</label>
            </h4>
            <h4 class="clearfix">
                <label>历史最高血压值</label>
                <div class="form_right historyNum_Box Num_Box" >
                    <input type="number" onkeyup="value=value.replace(/[^\d]/g,'')" class="historyNum flau_u_ro" placeholder="" value="<%=iH.highestNum%>"/>&nbsp;
                    /&nbsp;<input type="number" onkeyup="value=value.replace(/[^\d]/g,'')" class="historyNumlowNum"  placeholder="" value="<%=iH.lowNum%>"/>
                    <em>mmHg</em>
                </div>
            </h4>
            <h4 class="clearfix">
                <label>近期血压水平</label>
                <div class="form_right recentNum_Box Num_Box">
                    <input type="number"  class="recentNum flau_u_ro" placeholder="" value="<%=iHs.highestNum%>"/> /&nbsp;
                    <input type="number"  class="recentNumlowNum" placeholder="" value="<%=iHs.lowNum%>"/>
                    <em>mmHg</em>
                </div>
            </h4>
            <h4 class="clearfix form_block">
                <label>引起血压升高的原因</label>
                <div class="form_right">
                    <textarea contenteditable="true" name="sickReason" class="sickReason" placeholder="请输入引起血压升高的原因"><%=templetData.sickReason%></textarea>
                </div>
            </h4>

        </div>
        <div class="Teminput_item">
            <h4 class="clearfix item_title">
                <label>用药情况</label>
            </h4>
            <div class="selectH4 isTap2 isTap2YY"> <!--其他-->
                <h4 class="clearfix">
                    <label>药品名称</label>
                    <div class="form_right">
                        <input type="text" placeholder="请输入药品名称" name="medicineName" value="<%=templetData.medicineName%>"/>
                    </div>
                </h4>
                <h4 class="clearfix">
                    <label>用药剂量</label>
                    <div class="form_right">
                        <input type="number" placeholder="请输入用药剂量" name="medicineQuantity" value="<%=templetData.medicineQuantity%>"/>
                        <!--<em>mmol/l</em>-->
                    </div>
                </h4>
            </div>
        </div>
        <div class="Teminput_item">
            <h4 class="clearfix form_block">
                <label>健康建议</label>
                <i class="Jtips">Tips 》</i>
                <div class="form_right">
                    <textarea placeholder="请输入您对本次咨询的健康建议" contenteditable="true" name="advise" class="TNB_advTTextarea" value="<%=templetData.advise%>"><%=templetData.advise%></textarea>
                </div>
            </h4>
        </div>
    </form>
</div>
<% } else {%>
<section class="Ynav temNav">
    <dfn isHref="J_docAdviceList.html?memberId=<%=memberId%>" class="Y_tump adviceBack J_jumpBtnIback"> </dfn>
    <h1 class="category">高血压咨询模板</h1>
    <i class="temOk ifSaveBtn">发送</i>
    <span class="temSave ifSaveBtn">保存</span>
</section>
<div class="Y_docIndex JindexBox JindexBoxTop">
    <form class="TNB_listForm">
        <input type="hidden" name="applyFormId" value="<%=applyFormId%>"/>
        <div class="Teminput_item">
            <h4 class="clearfix">
                <label>问诊时间</label>
                <div class="form_right">
                    <input type="text" name="startTime " class="startTime ifNullInp" value=""/>
                    <em class="huiRight"></em>
                </div>
            </h4>
            <h4 class="clearfix">
                <label>患病年限</label>
                <div class="form_right">
                    <input type="number" name="sickYears" class="sickYears ifNullInp" placeholder="请输入患病年限" value=""/>
                    <em>年</em>
                </div>
            </h4>
            <h4 class="clearfix">
                <label>确诊医院</label>
                <div class="form_right">
                    <input type="text"  name="hospitalName"  class="hospitalName ifNullInp" placeholder="请输入确诊医院" value=""/>
                </div>
            </h4>
            <h4 class="clearfix form_block">
                <label>本次咨询原因</label>
                <div class="form_right">
                    <textarea name="askReason" contenteditable="true" value="" class="askReason ifNullInp"  placeholder="请输入咨询原因"></textarea>
                </div>
            </h4>
        </div>
        <div class="Teminput_item ">
            <h4 class="clearfix item_title">
                <label>血压情况</label>
            </h4>
            <h4 class="clearfix">
                <label>历史最高血压值</label>
                <div class="form_right historyNum_Box Num_Box" >
                    <input type="number"  class="historyNum flau_u_ro " placeholder="" value=""/>&nbsp;
                    /&nbsp;
                    <input type="number" class="historyNumlowNum"  placeholder="" value=""/>
                    <em>mmHg</em>
                </div>
            </h4>
            <h4 class="clearfix">
                <label>近期血压水平</label>
                <div class="form_right recentNum_Box Num_Box">
                    <input type="number" class="recentNum flau_u_ro" placeholder="" value=""/> &nbsp;
                    /&nbsp;
                    <input type="number" class="recentNumlowNum" placeholder="" value=""/>
                    <em>mmHg</em>
                </div>
            </h4>
            <h4 class="clearfix form_block">
                <label>引起血压升高的原因</label>
                <div class="form_right">
                    <textarea contenteditable="true" name="sickReason" class="sickReason" placeholder="请输入引起血压升高的原因"></textarea>
                </div>
            </h4>

        </div>
        <div class="Teminput_item">
            <h4 class="clearfix item_title">
                <label>用药情况</label>
            </h4>
            <div class="selectH4 isTap2 isTap2YY" > <!--其他-->
                <h4 class="clearfix">
                    <label>药品名称</label>
                    <div class="form_right">
                        <input type="text" placeholder="请输入药品名称" name="medicineName" value=""/>
                    </div>
                </h4>
                <h4 class="clearfix">
                    <label>用药剂量</label>
                    <div class="form_right">
                        <input type="number" placeholder="请输入用药剂量" name="medicineQuantity" value=""/>
                        <em>mmol/l</em>
                    </div>
                </h4>
            </div>
        </div>
        <div class="Teminput_item">
            <h4 class="clearfix form_block">
                <label>健康建议</label>
                <i class="Jtips">Tips >></i>
                <div class="form_right">
                    <textarea contenteditable="true" placeholder="请输入您对本次咨询的健康建议" name="advise" class="TNB_advTTextarea" value=""></textarea>
                </div>
            </h4>
        </div>
    </form>
</div>
<% } %>





