<div class="Y_MoreBFCon">
    <div class="Y_MoreBFConBot">
        <div class="BotType">
            <p class="BotType_o">体重</p>
            <p class="BotType_t"><%=data.bodyWeight%></p>
        </div>
        <div class="BotTp">
            <%if(data.bmi<18.5){%>
            <span style="color:#FFAB2B!important;"><%=data.analyzeFatType%></span>
            <%}else if(data.bmi >= 24.0){%>
            <span style="color:#FF6262!important;"><%=data.analyzeFatType%></span>
            <%}else{ %>
            <span style="color:#6590FF!important;"><%=data.analyzeFatType%></span>
            <%}%>
        </div>
        <div class="BotType">
            <p class="BotType_o">脂肪</p>
            <p class="BotType_t"><%=data.bodyfatRate%></p>
        </div>
    </div>
    <div class="Y_MoreBFConAny">
        <h2>分析报告</h2>
        <div class="report">
            <p class="title">核心指标</p>

