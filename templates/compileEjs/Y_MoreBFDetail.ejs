            <%if(data.listHightData.length == 0){%>
                <div class="listHightData" style="display: none">
                    <p class="warning"><%=data.listHightData.length%>项特别注意指标</p>
                    <div id="listHightData_list"></div>
                </div>
            <%}else{%>
                <div class="listHightData">
                    <p class="warning"><%=data.listHightData.length%>项特别注意指标</p>
                    <div id="listHightData_list"></div>
                </div>
            <%}%>

            <%if(data.listLowData.length == 0){%>
                <div class="listLowData" style="display: none">
                    <p class="warning_t"><%=data.listLowData.length%>项警告指标</p>
                    <div id="listLowData"></div>
                </div>
            <%}else{%>
                <div class="listLowData">
                    <p class="warning_t"><%=data.listLowData.length%>项警告指标</p>
                    <div id="listLowData"></div>
                </div>
            <%}%>

            <%if(data.listNormalData.length == 0){%>
                <div class="listNormalData" style="display: none">
                    <p class="warning_r"><%=data.listNormalData.length%>项放心指标</p>
                    <div id="listNormalData"></div>
                </div>
            <%}else{%>
                <div class="listNormalData">
                    <p class="warning_r"><%=data.listNormalData.length%>项放心指标</p>
                    <div id="listNormalData"></div>
                </div>
            <%}%>

            <p class="title_body">身体指标</p>
            <div class="body_target">
                <div class="target">
                    <div class="age">
                        <img src="../images/Group_21.png"/>
                        <span>身体年龄</span>
                    </div>
                    <div class="age_number">
                        <%=data.bodyAge%>
                    </div>
                </div>
                <div class="target">
                    <div class="age">
                        <img src="../images/Group_22.png"/>
                        <span>BMI</span>
                    </div>
                    <div class="age_number">
                        <%=data.bmi%>
                    </div>
                </div>
            </div>
        </div>
    </div>
